generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  senha String

  @@map("usuarios")
}

model Categoria {
  id        Int    @id @default(autoincrement())
  descricao String

  @@map("categorias")
}
